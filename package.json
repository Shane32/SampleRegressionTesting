{
  "name": "testapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^13.13.48",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-responsive": "^8.0.1",
    "@types/react-router-dom": "^5.1.7",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-compound-slider": "^2.5.0",
    "react-dom": "^16.14.0",
    "react-image-magnify": "^2.7.4",
    "react-intl": "^3.12.1",
    "react-responsive": "^8.2.0",
    "react-responsive-modal": "^4.0.1",
    "react-router-dom": "^5.2.0",
    "react-script-hook": "^1.3.0",
    "react-scripts": "^4.0.3",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": "^14.13.1",
    "npm": "^6.14.8"
  },
  "scripts": {
    "start": "rimraf ./build && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o storybook-static",
    "test-storybook": "build-storybook -s public -o storybook-static && react-scripts test",
    "update-snapshots": "build-storybook -s public -o storybook-static && react-scripts test -u",
    "test-update": "react-scripts test -u"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.3",
    "@storybook/addon-essentials": "^6.2.3",
    "@storybook/addon-links": "^6.2.3",
    "@storybook/addon-storyshots": "^6.2.3",
    "@storybook/addon-storyshots-puppeteer": "^6.2.3",
    "@storybook/node-logger": "^6.2.3",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.3",
    "puppeteer": "^8.0.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
